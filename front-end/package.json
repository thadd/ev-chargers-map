  "private": true,
  "dependencies": {
    "@apollo/react-hooks": "^3.1.1",
    "apollo-boost": "^0.4.4",
    "font-awesome": "^4.7.0",
    "graphql": "^14.5.7",
    "leaflet": "^1.5.1",
    "leaflet-geosearch": "^2.7.0",
    "leaflet.awesome-markers": "^2.0.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "^3.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
