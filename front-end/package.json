  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "bootstrap": "^4.3.1",
    "env-cmd": "^8.0.2",
    "jquery": "^3.4.1",
    "jsonwebtoken": "^8.5.1",
    "node-sass-chokidar": "^1.3.4",
    "node-sass-import": "^2.0.1",
    "node-sass-tilde-importer": "^1.0.2",
    "popper.js": "^1.15.0",
    "typescript": "^3.6.3"
