{
  "name": "ev-chargers",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.8.6",
    "react-apple-mapkitjs": "^0.2.4",
    "react-dom": "^16.8.6",
    "react-scripts": "2.1.8"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "update-token": "node get-map-token.js > src/token.json",
    "build-css": "npx node-sass-chokidar src/ -o src/ --importer=node_modules/node-sass-tilde-importer",
    "watch-css": "npm run build-css && npx node-sass-chokidar src/ -o src/ --watch --recursive --importer=node_modules/node-sass-tilde-importer"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "bootstrap": "^4.3.1",
    "env-cmd": "^8.0.2",
    "jsonwebtoken": "^8.5.1",
    "node-sass-chokidar": "^1.3.4",
    "node-sass-import": "^2.0.1",
    "node-sass-tilde-importer": "^1.0.2"
  }
}
