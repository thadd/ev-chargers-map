{
  "name": "ev-chargers",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "font-awesome": "^4.7.0",
    "leaflet": "^1.7.1",
    "leaflet-geosearch": "^2.7.0",
    "leaflet.awesome-markers": "^2.0.5",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-scripts": "^3.4.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build-css": "npx node-sass-chokidar src/ -o src/ --importer=node_modules/node-sass-tilde-importer",
    "watch-css": "npm run build-css && npx node-sass-chokidar src/ -o src/ --watch --recursive --importer=node_modules/node-sass-tilde-importer"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "bootstrap": "^4.6.0",
    "env-cmd": "^8.0.2",
    "jsonwebtoken": "^9.0.0",
    "node-sass-chokidar": "^1.5.0",
    "node-sass-import": "^2.0.1",
    "node-sass-tilde-importer": "^1.0.2"
  }
}
